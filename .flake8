[flake8]
max-line-length = 119
exclude =
    # Ignore all this files some are autogenerated by Django
    # others are for configuration purposes only 
    .gitignore,.git,
    */migrations/*, 
    __pycache__, *.py[co],
    db.sqlite3,
    env,  
    dist,
    build,

per-file-ignores =
    ./random_file.py:E501
    ./settings/local.py:F405,F403
    ./settings/prod.py:F405,F403,F401
    ./settings/staging.py:F405,F403
    ./accounts/apps.py:F40